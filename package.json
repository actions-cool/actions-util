{
  "private": false,
  "name": "actions-util",
  "version": "1.1.2",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "father-build",
    "format": "prettier --write src/*.ts",
    "format-check": "prettier --check src/*.ts",
    "lint": "eslint src/*.ts",
    "prepublishOnly": "npm run build && np --yolo --no-publish",
    "all": "yarn format-check && yarn lint && yarn build"
  },
  "author": "xrkffgg",
  "homepage": "https://github.com/actions-cool/actions-util",
  "keywords": [
    "action",
    "ts",
    "js",
    "tool",
    "util"
  ],
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions-cool/actions-util/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/actions-cool/actions-util",
    "branch": "main"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.5.6",
    "eslint": "^7.18.0",
    "father-build": "^1.19.1",
    "np": "^7.2.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  }
}
